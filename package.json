{
  "name": "github-cla-webhook",
  "private": true,
  "version": "0.0.0",
  "description": "Automate CLA verification for open source repositories using GitHub issues",
  "main": "github-cla-webhook.js",
  "scripts": {
    "lint": "standard",
    "test-spec": "mocha -R spec --bail",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec --bail",
    "test": "npm run lint && npm run test-cov",
    "start": "node ."
  },
  "standard": {
    "ignore": [
      "coverage/**",
      "node_modules/**",
      "bower_components/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mulesoft-labs/github-cla-webhook.git"
  },
  "keywords": [],
  "author": "MuleSoft, Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mulesoft-labs/github-cla-webhook/issues"
  },
  "homepage": "https://github.com/mulesoft-labs/github-cla-webhook",
  "engines": {
    "node": ">=0.12"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0",
    "pre-commit": "^1.0.1",
    "standard": "^2.1.1"
  },
  "dependencies": {
    "body-parser": "^1.12.0",
    "debug": "^2.1.2",
    "express": "^4.12.2",
    "limits.js": "^0.1.4",
    "lru-cache": "^2.5.0",
    "parse-link-header": "^0.2.0",
    "popsicle": "^0.3.11",
    "popsicle-basic-auth": "0.0.1",
    "popsicle-constants": "0.0.1",
    "popsicle-prefix": "0.0.1",
    "popsicle-status": "0.0.2"
  }
}
